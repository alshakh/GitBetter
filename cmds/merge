
__run__() {
    gb -is-in-repo || return 1
    [[ "$1" == "!" ]] && {
        shift
    }
    git merge "$@"
}

__complete__() {
    local cur=$(sed 's/^\^//' <<< "${!#}")
    local posss=$(gb -list-refs --no-relatives --no-tags)
    compgen -W "$posss" -- $cur

    return 0
}

__usage__() {
    return 0
}
