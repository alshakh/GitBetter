
__run__() {
    gb -is-in-repo || return 1

    git pull "$@"
}

__complete__() {
    if (( $# == 0 )) ; then
        compgen -W "$(gb -list-remotes)" -- "$CURRENT_TERM"
    elif (( $# > 0 )) ; then
        local ps=$(gb -list-refs --only-remotes | sed "s@$1/@@")
        compgen -W "$ps" -- "$CURRENT_TERM"
    fi
}

__usage__() {
    return 0
}
