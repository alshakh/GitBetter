
__run__() {
    gb -is-in-repo || return 1

    if $SHOUTING ; then
        git push -f "$@"
    else
        git push "$@"
    fi
}

__complete__() {
    if (( $# == 0 )) ; then
        compgen -W "$(gb -list-remotes)" -- "$CURRENT_TERM"
    elif (( $# > 0 )) ; then
        compgen -W "$(gb -list-refs --no-relatives)" -- "$CURRENT_TERM"
    fi
}

__usage__() {
    return 0
}
